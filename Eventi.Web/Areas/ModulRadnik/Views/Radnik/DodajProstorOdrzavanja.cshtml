@model Eventi.Web.Areas.ModulRadnik.Models.DodajProstorOdrzavanjaVM
@{
    ViewData["Title"] = "DodajProstorOdrzavanja";
}

<h1>DodajProstorOdrzavanja</h1>

<form method="post">

    <div class="form-group">
        <label asp-for="Naziv"></label>
        <input type="text" asp-for="Naziv" />
    </div>

    <div class="form-group">
        <label asp-for="GradOdabir"></label>
        <select asp-for="GradOdabir" asp-items="@Model.Gradovi"></select>
    </div>

    <div class="form-group">
        <label asp-for="TipProstoraOdabir"></label>
        <select asp-for="TipProstoraOdabir" asp-items="@Model.TipProstori"></select>
    </div>

    <div class="form-group">
        <label asp-for="Adresa"></label>
        <input type="text" asp-for="Adresa" />
    </div>



    <div class="form-group">
        <input type="submit" value="Potvrdi" />
    </div>
</form>