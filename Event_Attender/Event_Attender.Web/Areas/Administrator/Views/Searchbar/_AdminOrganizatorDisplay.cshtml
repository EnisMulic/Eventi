@using Event_Attender.Data.Models
@{
    ViewData["Title"] = "Index";
    List<Organizator> Organizatori = (List<Organizator>)ViewData["Organizatori"];
    Layout = "_Layout";
}

<link href="~/css/admin-sidebar.css" rel="stylesheet" />

<div id="Admin-Container">
    @{await Html.RenderPartialAsync("_AdminSidebar");}
    <div id="Admin-Searchbar">
        <input type="text" name="search" value="" />
        <table>
            <tbody>
                @foreach (Organizator o in Organizatori)
                {
                    <tr>
                        <td>@o.Naziv</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
    <div id="Main-Display">

    </div>
</div>