@using Event_Attender.Data.Models
@{
    ViewData["Title"] = "Index";
    List<Korisnik> Korisnici = (List<Korisnik>)ViewData["Korisnici"];
    Layout = "_Layout";
}

<link href="~/css/admin-sidebar.css" rel="stylesheet" />

<div id="Admin-Container">
    @{await Html.RenderPartialAsync("_AdminSidebar");}
    <div id="Admin-Searchbar">
        <input type="text" name="search" value="" />
        <table>
            <tbody>
                @foreach (Korisnik k in Korisnici)
                {
                    <tr>
                        <td>@k.Osoba.Prezime + " " + @k.Osoba.Ime</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
    <div id="Main-Display">

    </div>
</div>